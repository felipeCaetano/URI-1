int main (int argc, char * argv []) {
	int A, horas;
	duplo B;	
	scanf ("% d", & A);
	scanf ("% lf", & horas);
	scanf ("% d", & B);
	printf ("NÚMERO =% d \ n", A);
	if (horas> 0) {
		printf ("SALÁRIO = U $% 0.2lf \ n", B * horas);	
	}outro{
		printf ("ERRO!");
}
	
	retornar 0;
}
